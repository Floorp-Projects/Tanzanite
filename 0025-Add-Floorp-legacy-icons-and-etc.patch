+			stroke-dasharray: var(--radius);
+			stroke: context-fill;
+			stroke-width: 12.5%;
+			stroke-linecap: round;
+			transform-origin: center;
+			animation: stroke-rotate calc(1.333s * 4) cubic-bezier(.35, 0, .25, 1) infinite both;
+		}
