+  file, You can obtain one at http://mozilla.org/MPL/2.0/.
+ */
+
+
+html, body {
+  padding: 0;
+  margin: 0;
+  width: 100%;
+  height: 100%;
+}
+
+.icon {
+  -webkit-user-select: none;
+  user-select: none;
+  display: inline-block;
+}
+
+
+.hidden {
+  display: none;
+}
+
+
+/* Offline page */
+
+.offline .interstitial-wrapper {
+  color: #2b2b2b;
+  font-size: 1em;
+  line-height: 1.55;
+  margin: 0 auto;
+  max-width: 600px;
+  padding-top: 100px;
+  width: 100%;
+}
+
+#main-frame-error {
+  transform: scale(2 , 2);
+}
+
+.offline .runner-container {
+  height: 150px;
+  max-width: 600px;
+  overflow: hidden;
+  position: absolute;
+  top: auto;
+  width: 44px;
+}
+
+#offline-resources {
+  display: none;
+}
+
+@media (prefers-color-scheme: dark) {
+  body {
+    background-color: rgb(32, 33, 36);
+    color: #fff;
+  }
+  .runner-canvas {
+      filter: invert(1);
+  }
+}
