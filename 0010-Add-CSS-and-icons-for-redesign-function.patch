+#appMenu-proton-update-banner,
+#appMenu-quit-button2.subviewbutton{ order: 4 }
+
+
+#appMenu-more-button2{ min-width: 170px }
+#appMenu-more-button2 > .toolbarbutton-icon{ -moz-box-flex: 0 !important; }
