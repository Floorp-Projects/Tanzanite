+      /* border-radius: 0 0 8px 8px !important; */
+    }
+
+    /*- Button -----------------------------------------------------------------*/
+    button {
+      -moz-appearance: none !important;
+      color: var(--in-content-button-text-color) !important;
+      background-color: var(--in-content-button-background) !important;
+      font: inherit;
